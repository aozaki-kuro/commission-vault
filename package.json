{
  "name": "nsfw-commission-neo",
  "version": "1.2.0",
  "private": true,
  "author": "Crystallize",
  "scripts": {
    "build": "bun run pre-build && next build",
    "clean": "rm -rf .next node_modules out .wrangler && bun i",
    "deploy": "bun run prod && wrangler deploy",
    "dev": "bun run pre-build && next dev --turbo",
    "dl": "bun run scripts/dl.ts",
    "lint": "eslint --fix",
    "pre-build": "bun run scripts/convert.ts && bun run scripts/imageImport.ts",
    "prepare": "husky",
    "prod": "bun run pre-build && next build",
    "start": "next start"
  },
  "lint-staged": {
    "*.{js,mjs,cjs,jsx,ts,tsx}": [
      "bun lint"
    ],
    "*.{json,css,md,mdx,jsonc}": [
      "bun prettier --write"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "dotenv": "^17.2.1",
    "next": "^15.4.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rss-parser": "^3.13.0",
    "sharp": "^0.34.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/eslint__eslintrc": "^3.3.0",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.32.0",
    "eslint-config-next": "15.4.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "wrangler": "^4.26.1"
  }
}
