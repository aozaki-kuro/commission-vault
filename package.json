{
  "name": "nsfw-commission-neo",
  "author": "Crystallize",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "dev": "bun run pre-build && astro dev",
    "pre-build": "bun run scripts/convert.ts && bun run scripts/imageImport.ts",
    "dl": "bun run scripts/dl.ts",
    "build": "bun run pre-build && astro build",
    "prod": "bun run dl && bun run pre-build && astro build",
    "start": "astro preview",
    "lint": "eslint --fix",
    "prepare": "husky",
    "clean": "rm -rf .next node_modules out .wrangler && bun i"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "rss-parser": "^3.13.0",
    "sharp": "^0.34.2",
    "astro": "^4.8.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.10",
    "@types/eslint__eslintrc": "^3.3.0",
    "@types/node": "^24.0.3",
    "autoprefixer": "^10.4.21",
    "@astrojs/tailwind": "^5.0.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3",
    "wrangler": "^4.22.0"
  },
  "lint-staged": {
    "*.{js,mjs,cjs,jsx,ts,tsx}": [
      "bun lint"
    ],
    "*.{json,css,md,mdx,jsonc}": [
      "bun prettier --write"
    ]
  }
}
