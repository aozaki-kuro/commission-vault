{
  "name": "nsfw-commission-neo",
  "author": "Crystallize",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "dev": "bun run pre-build && next dev --turbo",
    "pre-build": "bun run scripts/convert.ts && bun run scripts/imageImport.ts",
    "dl": "bun run scripts/dl.ts",
    "build": "bun run pre-build && next build",
    "prod": "bun run dl && bun run pre-build && next build",
    "start": "next start",
    "lint": "eslint --fix",
    "prepare": "husky",
    "clean": "rm -rf .next node_modules out .wrangler && bun i"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "dotenv": "^16.5.0",
    "next": "^15.3.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rss-parser": "^3.13.0",
    "sharp": "^0.34.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.8",
    "@types/eslint__eslintrc": "^3.3.0",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.28.0",
    "eslint-config-next": "15.3.4",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "*.{js,mjs,cjs,jsx,ts,tsx}": [
      "bun lint"
    ],
    "*.{json,css,md,mdx,jsonc}": [
      "bun prettier --write"
    ]
  }
}
