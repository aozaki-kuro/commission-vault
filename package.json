{
  "name": "nsfw-commission-neo",
  "author": "Crystallize",
  "version": "1.0.2",
  "private": true,
  "scripts": {
    "dev": "bun run pre-build && next --turbo",
    "pre-build": "bun run scripts/convert.ts && bun run scripts/imageImport.ts",
    "dl": "bun run scripts/dl.ts",
    "build": "bun run pre-build && next build",
    "prod": "bun run dl && bun run pre-build && next build",
    "start": "next start",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "dotenv": "^16.4.7",
    "next": "^15.1.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rss-parser": "^3.13.0",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "eslint": "^9",
    "eslint-config-next": "15.1.2",
    "@eslint/eslintrc": "^3",
    "@next/env": "^15.1.2",
    "autoprefixer": "^10.4.20",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9"
  },
  "lint-staged": {
    "*.{js,mjs,cjs,jsx,ts,tsx}": [
      "eslint --cache --fix --cache-location './node_modules/@eslint/.eslintcache/' --flag unstable_ts_config --config eslint.config.ts"
    ],
    "*.{json,css,md,mdx}": [
      "prettier --write"
    ]
  }
}
