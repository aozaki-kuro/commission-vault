{
  "name": "nsfw-commission-neo",
  "author": "Crystallize",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "dev": "bun run pre-build && next dev --turbo",
    "pre-build": "bun run scripts/convert.ts && bun run scripts/imageImport.ts",
    "dl": "bun run scripts/dl.ts",
    "build": "bun run pre-build && next build",
    "prod": "bun run dl && bun run pre-build && next build",
    "start": "next start",
    "lint": "eslint --fix",
    "prepare": "husky",
    "clean": "rm -rf .next node_modules out .wrangler && bun i"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "dotenv": "^16.4.7",
    "next": "^15.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rss-parser": "^3.13.0",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@tailwindcss/postcss": "^4.0.8",
    "@types/eslint__eslintrc": "^2.1.2",
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.21.0",
    "eslint-config-next": "15.2.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.8",
    "typescript": "^5.7.3"
  },
  "lint-staged": {
    "*.{js,mjs,cjs,jsx,ts,tsx}": [
      "bun lint"
    ],
    "*.{json,css,md,mdx,jsonc}": [
      "bun prettier --write"
    ]
  }
}
